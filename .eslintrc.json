{
    "env": {
        "commonjs": true,
        "es2021": true,
        "node": true
    },
    "extends": "google",
    "overrides": [],
    "parserOptions": {
        "ecmaVersion": "latest"
    },
    "rules": {
        "require-jsdoc": "off",
        "valid-jsdoc": "off",
        "max-len": "off",
        "no-tabs": "off",
        "prefer-promise-reject-errors": "off",
        "no-mixed-spaces-and-tabs": "off"
    }
}